#include<stdio.h>
int main()
{
    int arr[]={12,23,42},i;
    int max=arr[0],min=arr[0];
    int length=sizeof(arr)/sizeof(arr[0]);
    for(i=1;i<length;i++)
    {
        if(arr[i]>max)
        max=arr[i];
        if(arr[i]<min)
        min=arr[i];
    }
    printf("maximum number in a given array %d\n",max);
    printf("minimum number in a given array %d",min);
    return 0;
}
